    "@react-three/drei": "^9.48.2",
    "@react-three/fiber": "^8.9.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "framer-motion": "^6",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1",
    "three": "^0.148.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/three": "^0.146.0",
    "eslint-config-prettier": "^8.5.0",
